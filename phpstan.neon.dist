includes:
  - ./vendor/nunomaduro/larastan/extension.neon

parameters:
  paths:
    - app
    - bootstrap
    - config
    - database
    - routes
    - tests

  level: 5

  ignoreErrors:
    - '#Undefined variable: \$this#'
    - '#Call to an undefined method [a-zA-Z\\\]+::[a-zA-Z0-9_]+#'

  scanFiles:
    - bootstrap/app.php

